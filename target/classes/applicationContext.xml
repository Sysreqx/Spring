<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="musicBean"
          class="com.kaisar.springApp1.RockMusic">
    </bean>

    <bean id="musicPlayer"
          class="com.kaisar.springApp1.MusicPlayer">
        <!--В поле name указываем имя сеттера setMusic, автоматически удаляет set, переводит первую букву после set в нижний регистр-->
        <property name="music" ref="musicBean"></property>
    </bean>

</beans>